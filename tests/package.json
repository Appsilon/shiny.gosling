{
  "name": "cypress",
  "version": "0.0.1",
  "main": "index.js",
  "license": "MIT",
  "devDependencies": {
    "cypress": "^12.17.2",
    "cypress-plugin-snapshots": "^1.4.4",
    "start-server-and-test": "^2.0.0"
  },
  "scripts": {
    "installDependencies": "Rscript cypress/r_scripts/install_dependencies.R",
    "circularLinearWithBrush": "Rscript cypress/r_scripts/run_circular_linear_with_brush.R",
    "barChart": "Rscript cypress/r_scripts/run_barchart.R",
    "areaChart": "Rscript cypress/r_scripts/run_area_chart.R",
    "e2e-circularLinearWithBrush": "start-server-and-test circularLinearWithBrush http://localhost:8888 'cypress run --spec \"cypress/e2e/circularLinearWithBrushSpec.cy.js\" --record false'",
    "e2e-barChart": "start-server-and-test barChart http://localhost:8888 'cypress run --spec \"cypress/e2e/barChart.cy.js\" --record false'",
    "e2e-areaChart": "start-server-and-test areaChart http://localhost:8888 'cypress run --spec \"cypress/e2e/areaChart.cy.js\" --record false'"
